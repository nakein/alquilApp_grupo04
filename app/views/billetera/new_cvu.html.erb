<h1>Agregar nuevo CVU</h1>
    <%= form_with url: '/billetera/agregar_cvu', method: :patch do |form| %>

        <div class="field">
        <%= form.label "Nombre:" %>
        <%= form.text_field :name, required: true%>
        </div>

        <div class="field">
        <%= form.label "Digitos:" %>
        <%= form.text_field :digits, required: true%>
        </div>

        <div class="field">
        <%= form.label "Alias:" %>
        <%= form.text_field :alias, required: true%>
        </div>

        <%= form.submit "Agregar CVU", :class=>"btn btn-outline-warning my-4"%>
    <% end %>  